--VERIFY DEVICE--


function VerifyDevice()
        if UserInputService.KeyboardEnabled then
                GetDevice = "Computer"
        else if UserInputService.TouchEnabled then
                        GetDevice = "Phone"
                else if UserInputService.GamepadEnabled then
                                GetDevice = "Console"
                        else if GetDevice == "" then
                                        GetDevice = "Unkown"
                                end
                        end
                end
        end
end


--VERIFY DEVICE--


