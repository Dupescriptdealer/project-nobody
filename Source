--BYPASS SPAM LOG--


function BypassSpamLog()
    if bypassshittyspamlog == true then
        local saveintocommands = biggamescommands
        if saveintocommands.Text == "BIG Games Commands" then
            saveintocommands.Text = "Executed"
        else
            dontsendlog = true
        end
    end
end


--BYPASS SPAM LOG--


